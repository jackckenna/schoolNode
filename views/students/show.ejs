<table border="1" class="qa-show-user">
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Year</th>
    <th>Options</th>
  </tr>
  <tr id="<%= student.id%>">
    <td><%=student.firstname%></td>
    <td><%= student.secondname%></td>
    <td><%= student.Year%></td>
    <td><a href="/students/<%= student.id%>/edit"> Edit Student</a></td>
    <td>
      <form action="/students/<%= student.id %>" method="POST">
      <input type="hidden" name="_method" value="DELETE">
      <input type="submit" value="Delete" id="delete" >
      </form> 
    </td>
  </tr>
</table>